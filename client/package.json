{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "homepage": ".",
  "dependencies": {
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "antd": "^5.18.1",
    "axios": "^1.7.2",
    "buffer": "^6.0.3",
    "js-yaml": "^4.1.0",
    "localforage": "^1.10.0",
    "nth-check": "^2.1.1",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "react-scripts": "5.0.1",
    "utif": "^3.1.0",
    "web-vitals": "^4.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "cross-env CI=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "dist": "electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.16.7",
    "cross-env": "^7.0.3",
    "electron": "^27.3.11",
    "electron-builder": "^24.13.3",
    "prettier": "^3.3.2"
  },
  "build": {
    "appId": "com.example.pytcclient",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "../dist/pytc_backend",
        "to": "pytc_backend"
      }
    ]
  },
  "overrides": {
    "nth-check": "$nth-check",
    "resolve-url-loader": "^5.0.0",
    "svgo": "^3.3.2"
  }
}
